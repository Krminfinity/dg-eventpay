PORT=4000

# KOMOJU credentials (Sandbox or Live)
## IMPORTANT: 実キーは絶対にコミットしないでください。以下はダミーのプレースホルダです。
# Backend secret key (keep private; do NOT commit real values)
KOMOJU_SECRET_KEY=__YOUR_KOMOJU_SECRET_KEY__
# Frontend publishable key (used by client if needed)
KOMOJU_PUBLIC_KEY=__YOUR_KOMOJU_PUBLIC_KEY__
# Optional: OAuth/Connect style apps may need this
KOMOJU_CLIENT_ID=__YOUR_KOMOJU_CLIENT_ID__
# API version to target (optional; follow your account setting)
KOMOJU_API_VERSION=2025-01-28

# Webhook signing secret from KOMOJU dashboard (Webhooks settings)
KOMOJU_WEBHOOK_SECRET=__YOUR_KOMOJU_WEBHOOK_SECRET__
# 初回の接続確認などで署名検証を一時的にスキップする（本番は必ず false）
KOMOJU_WEBHOOK_SKIP_VERIFY=false
